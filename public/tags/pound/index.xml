width="1600"
height="900"
srcset="https://dartmoorwalking.org/p/ingra-tor/3_hu_177f0c04a563fa19.jpg 480w, https://dartmoorwalking.org/p/ingra-tor/3_hu_a3f5dc6da2d8dc4f.jpg 1024w"
loading="lazy"
