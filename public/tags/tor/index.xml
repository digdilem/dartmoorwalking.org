	loading=&#34;lazy&#34;
	
		alt=&#34;Unusual granite rail showing tare and feather marks. These are usually placed down, so one supposes this has them on both sides&#34;
	
	
		class=&#34;gallery-image&#34; 
		data-flex-grow=&#34;177&#34;
		data-flex-basis=&#34;426px&#34;
	
&gt;&lt;/p&gt;
